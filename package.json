  "scripts": {
    "build": "rimraf ./dist && mkdir -p dist && ts-node ./pug/build.ts && rollup --config rollup.config.mjs && typedoc && touch ./dist/.nojekyll",
    "dev:docs": "nodemon --watch src --ext ts,json --exec typedoc",
    "dev:https": "ts-node ./https.ts",
    "dev:ts": "nodemon --watch src --ext ts,json --exec \"rollup --config rollup.config.mjs\"",
    "dev:pug": "nodemon --watch pug --ext ts,json ./pug/build.ts",
    "dev": "DEBUG_COLORS=1 concurrently --names \"TS,PUG,DOC,HTTPS\" -c \"bgBlue,bgYellow,bgGray,bgGreen\" \"yarn dev:ts\" \"yarn dev:pug\" \"yarn dev:docs\" \"yarn dev:https\"",
    "docs": "typedoc",
    "lint:ci": "eslint --ext .mjs,.cjs,.js,.ts,.pug .",
    "lint": "yarn lint:ci --fix",
    "mkcert": "mkdir ./mkcert && mkcert -key-file ./mkcert/key.pem -cert-file ./mkcert/cert.pem -ecdsa localhost",
    "test": "jest --roots ./src --"
  }
}
