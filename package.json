  },
  "contributors": [
    {
      "email": "taichunmin@gmail.com",
      "name": "Chunmin Tai",
      "url": "https://taichunmin.idv.tw"
    }
  ],
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "serialport": "^12.0.0",
    "web-serial-polyfill": "^1.0.14",
    "webbluetooth": "^3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@tsconfig/node-lts": "^18.12.5",
    "@types/debug": "^4.1.8",
    "@types/finalhandler": "^1.2.1",
    "@types/html-minifier": "^4.0.3",
    "@types/jest": "^29.5.5",
    "@types/livereload": "^0.9.3",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.3",
    "@types/pug": "^2.0.6",
    "@types/serve-static": "^1.15.2",
    "@types/uglify-js": "^3.17.2",
    "@types/web-bluetooth": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-pug": "^1.2.5",
    "finalhandler": "^1.2.0",
    "html-minifier": "^4.0.0",
    "jest": "^29.7.0",
    "jstransformer-sass": "^1.0.0",
    "livereload": "^0.9.3",
    "node-watch": "^0.7.4",
    "nodemon": "^3.0.1",
    "pug": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-version-injector": "^1.3.3",
    "serve-static": "^1.15.0",
    "supports-color": "^9.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typedoc-plugin-mdn-links": "^3.1.0",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typedoc-plugin-rename-defaults": "^0.6.6",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "utility-types": "^3.10.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": "./dist/iife/index.min.js",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./Crypto1": {
      "browser": "./dist/iife/Crypto1.min.js",
      "import": "./dist/es/Crypto1.mjs",
      "require": "./dist/cjs/Crypto1.cjs",
      "types": "./dist/types/Crypto1.d.ts"
    },
    "./buffer": {
      "browser": "./dist/iife/buffer.min.js",
      "import": "./dist/es/buffer.mjs",
      "require": "./dist/cjs/buffer.cjs",
      "types": "./dist/types/buffer.d.ts"
    },
    "./plugin/SerialPortAdapter": {
      "import": "./dist/es/plugin/SerialPortAdapter.mjs",
      "require": "./dist/cjs/plugin/SerialPortAdapter.cjs",
      "types": "./dist/types/plugin/SerialPortAdapter.d.ts"
    },
    "./plugin/WebbleAdapter": {
      "browser": "./dist/iife/plugin/WebbleAdapter.min.js",
      "import": "./dist/es/plugin/WebbleAdapter.mjs",
      "require": "./dist/cjs/plugin/WebbleAdapter.cjs",
      "types": "./dist/types/plugin/WebbleAdapter.d.ts"
    },
    "./plugin/WebserialAdapter": {
      "browser": "./dist/iife/plugin/WebserialAdapter.min.js",
      "import": "./dist/es/plugin/WebserialAdapter.mjs",
      "require": "./dist/cjs/plugin/WebserialAdapter.cjs",
      "types": "./dist/types/plugin/WebserialAdapter.d.ts"
    }
  },
  "keywords": [
    "browser",
    "chameleon-ultra",
    "chrome",
    "COM",
    "mifare classic",
    "nfc",
    "RFID",
    "serial port",
    "serial",
    "serialport",
    "UART",
    "web bluetooth",
    "web serial",
    "web-bluetooth-api",
    "web-serial-api"
  ],
  "resolutions": {
    "**/jstransformer-scss": "^2.0.0"
  },
  "scripts": {
    "build": "set -ex && rimraf ./dist && mkdir -p dist && cp -r public/* dist && yarn build:dts && ts-node ./pug/build.ts && rollup --config rollup.config.mjs && yarn docs && ts-node ./sitemap.ts && touch ./dist/.nojekyll",
    "build:dts": "tsc -p tsconfig.types.json --emitDeclarationOnly",
    "dev": "DEBUG_COLORS=1 concurrently --names \"DOC,HTTPS,PUBLIC,PUG,TS\" -c \"bgGray,bgGreen,bgGray,bgBlue,bgYellow\" \"yarn dev:docs\" \"yarn dev:https\" \"yarn dev:public\" \"yarn dev:pug\" \"yarn dev:ts\"",
    "dev:docs": "nodemon --watch src --watch pages --ext ts,md --exec \"yarn docs\"",
    "dev:https": "ts-node ./https.ts",
    "dev:public": "nodemon --watch public --ext svg,png,jpg,jpeg --exec \"cp -r public/* dist\"",
    "dev:pug": "nodemon --watch pug --ext pug ./pug/build.ts",
    "dev:ts": "nodemon --watch src --ext ts,json --exec \"rollup --config rollup.config.mjs\"",
    "docs": "typedoc --readme pages/typedoc.md",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint --ext .mjs,.cjs,.js,.ts,.pug .",
    "mkcert": "mkdir ./mkcert && mkcert -key-file ./mkcert/key.pem -cert-file ./mkcert/cert.pem -ecdsa localhost",
    "publish:test": "npm publish --dry-run",
    "test": "jest --roots ./src --",
    "test:ci": "jest --roots ./src --ci --coverage"
  }
}
